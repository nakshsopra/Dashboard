<mvc:View
    xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"
    xmlns:smarttable="sap.ui.comp.smarttable"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    controllerName="com.ss.jpb.rp.controller.Role"
>
    <Page
        id="_IDGenPage1"
        title="{i18n>Roles}"
        showNavButton="true"
        navButtonPress="onBack"
    >
        <content>
            <SplitApp id="id-role-splitapp">
                <masterPages>
                    <List
                        mode="MultiSelect"
                        id="idList"
                        selectionChange="onSelectItem"
                        growingScrollToLoad="true"
                        items="{path: '/RoleEntity'}"
                    >
                        <items>
                            <ObjectListItem id="_IDGenObjectListItem1"
                                title="{name_en_US}"
                            />
                        </items>
                    </List>
                </masterPages>
            </SplitApp>
        </content>
    </Page>
</mvc:View>
