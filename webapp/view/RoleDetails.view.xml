<mvc:View
    xmlns:table="sap.ui.table"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    controllerName="com.ss.jpb.rp.controller.RoleDetails"
>
    <Page
        id="_IDGenPage1"
        title="Role Details"
    >
        <ObjectHeader
            id="_IDGenObjectHeader1"
            title="{i18n>compareRoles}"
        />
        <content>
            <!--             <table:Table id="myTable" rows="{jsonModel>/items}"  >
                <table:columns>
                    <table:Column id="_IDGenColumn1" >
                        <Text id="_IDGenText1" text="Job roles"></Text>
                    </table:Column>
                    <table:Column id="_IDGenColumn2" >
                        <Text id="_IDGenText2" text="Job code"></Text>
                    </table:Column>
                    <table:Column id="_IDGenColumn3" >
                        <Text id="_IDGenText3" text="Competency"></Text>
                    </table:Column>
                    <table:Column id="_IDGenColumn4" >
                        <Text id="_IDGenText4" text="Job description"></Text>
                    </table:Column>
                </table:columns>
                <table:rows>
                </table:rows>
            </table:Table> -->
            <!-- Define the table -->
            <IconTabBar id="_IDGenIconTabBar1">
                <items>
                    <!-- Tab1 -->
                    <IconTabFilter
                        id="_IDGenIconTabFilter1"
                        text="Code"
                    >
                        <content>
                            <Table
                                id="myTable"
                                items="{roles>/jobCodes}"
                            >
                                <columns>
                                    <Column
                                        id="_IDGenColumn1"
                                        mergeDuplicates="true"
                                    >
                                        <Text
                                            id="_IDGenText1"
                                            text="Job role"
                                        />
                                    </Column>
                                    <Column id="_IDGenColumn2">
                                        <Text
                                            id="_IDGenText2"
                                            text="Job code"
                                        />
                                    </Column>
                                    <!-- <Column id="_IDGenColumn3">
                        <Text id="_IDGenText5" text="Competency" />
                    </Column>
                    <Column id="_IDGenColumn4">
                        <Text id="_IDGenText6" text="Job description" />
                    </Column> -->
                                </columns>
                                <items>
                                    <ColumnListItem id="_IDGenColumnListItem1">
                                        <cells>
                                            <Text
                                                id="_IDGenText3"
                                                text="{roles>name}"
                                            />
                                            <Text
                                                id="_IDGenText4"
                                                text="{roles>jobCode}"
                                            />
                                            <!-- <Text id="_IDGenText7" text="{roleCompetencyMappings/rating_en_US}"></Text>
                            <Text id="_IDGenText8" text="{}"></Text> -->
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </content>
                    </IconTabFilter>
                    <!-- Tab2 -->
                    <IconTabFilter
                        id="_IDGenIconTabFilter2"
                        text="Competencies"
                    >
                        <Table
                            id="CompetencyTable"
                            items="{roles>/jobCompetencies}"
                        >
                            <columns>
                                <Column
                                    id="_IDGenColumn3"
                                    mergeDuplicates="true"
                                >
                                    <Text
                                        id="_IDGenText5"
                                        text="Job role"
                                    />
                                </Column>
                                <Column id="_IDGenColumn4">
                                    <Text
                                        id="_IDGenText6"
                                        text="Competency"
                                    />
                                </Column>
                                <!-- <Column id="_IDGenColumn3">
                        <Text id="_IDGenText5" text="Competency" />
                    </Column>
                    <Column id="_IDGenColumn4">
                        <Text id="_IDGenText6" text="Job description" />
                    </Column> -->
                            </columns>
                            <items>
                                <ColumnListItem id="_IDGenColumnListItem2">
                                    <cells>
                                        <Text
                                            id="_IDGenText7"
                                            text="{roles>name}"
                                        />
                                        <Text
                                            id="_IDGenText8"
                                            text="{roles>jobCompetency}"
                                        />
                                        <!-- <Text id="_IDGenText7" text="{roleCompetencyMappings/rating_en_US}"></Text>
                            <Text id="_IDGenText8" text="{}"></Text> -->
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </IconTabFilter>
                    <!-- Tab3 -->
                    <IconTabFilter
                        id="_IDGenIconTabFilter3"
                        text="Description"
                    />
                </items>
            </IconTabBar>
        </content>
    </Page>
</mvc:View>
